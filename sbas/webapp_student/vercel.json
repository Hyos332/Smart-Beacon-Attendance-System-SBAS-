{
  "version": 2,
  "builds": [
    {
      "src": "src/index.tsx",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpeg|jpg|svg|gif))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url",
    "REACT_APP_ENV": "production",
    "REACT_APP_BEACON_ID": "@react_app_beacon_id",
    "REACT_APP_SERVICE_UUID": "@react_app_service_uuid"
  }
}